<div class="margin_20"></div>
<div class="content_container main_container mobile_padding pages_text">
    <div class="row">
        <div class="col-md-12">
            <div id="mm_page_content">
                Content
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var pathArray = window.location.pathname.split( '/' );
            var slug = pathArray[pathArray.length-1];
            prefix = get_prefix();
            var pages_json = prefix + ".json"
            $.getJSON(pages_json).done(function(data) {
                $('#mm_page_title').html(data.title.toUpperCase());
                $('#mm_page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            show_content();
        }
        loadMallDataCached(renderAll); 
    })
</script>
    
      
